-- Notificação utilitária
local function notify(titleText, text)
    pcall(function()
        game:GetService("StarterGui"):SetCore("SendNotification", {
            Title = titleText or "Notificação";
            Text = text or "";
            Duration = 3;
        })
    end)
end

-- Ação do botão
button.MouseButton1Click:Connect(function()
    local ok, err = pcall(function()
        if setclipboard then
            setclipboard(URL)
        else
            error("setclipboard não disponível")
        end
    end)

    if ok then
        notify("Link copiado", "O link foi copiado para a área de transferência.")
    else
        fallbackBox.Visible = true
        fallbackBox.Text = URL
        fallbackBox:CaptureFocus()
        pcall(function()
            fallbackBox.SelectionStart = 1
            fallbackBox.SelectionLength = #URL
        end)
        notify("Copiar manualmente", "Pressione Ctrl+C para copiar o link.")
        delay(6, function()
            if fallbackBox then fallbackBox.Visible = false end
        end)
    end
end)
